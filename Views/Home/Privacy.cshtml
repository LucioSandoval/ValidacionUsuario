@model User
@{
    ViewData["Title"] = "Resultado";
}

<h1>@ViewData["Title"]</h1>

<div>
    <h2>Datos del Usuario:</h2>
    <ul>
        <li><strong>Nombre:</strong> @Model.Nombre</li>
        <li><strong>Apellidos:</strong> @Model.Apellidos</li>
        <li><strong>Edad:</strong> @Model.Edad</li>
        <li><strong>CorreO Electrónico:</strong> @Model.CorreoElectronico</li>
        <li><strong>Contraseña:</strong> @Model.Contrasena</li>
    </ul>
</div>
